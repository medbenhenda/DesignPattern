<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>{{ title|default('Design pattern PHP') }}</title>
        <meta name="description" content="">
        {% block styles %}
            <link href="assets/css/bootstrap.min.css" rel="stylesheet">
            <!-- Custom CSS -->
            <link href="assets/css/thumbnail-gallery.css" rel="stylesheet">
            <link href="assets/css/thumbnail-gallery.css" rel="stylesheet">

        {% endblock %}
        <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
        <![endif]-->
        {{ debugbar('renderHead')|raw }}
        <link rel="stylesheet" href="assets/highlight/styles/androidstudio.css">

    </head>
    <body>
        {% include '_templates/header.twig' %}        
        <div class="container">
            {% block content %}
            {% endblock %}
            {% include '_templates/footer.twig' %}
        </div>
        <!-- /.container -->
        {% block scripts %}
            <!-- jQuery -->
            <script src="assets/js/jquery.js"></script>

            <!-- Bootstrap Core JavaScript -->
            <script src="assets/js/bootstrap.min.js"></script>
            <script src="bower_components/zeroclipboard/dist/ZeroClipboard.js"></script>
            <script src="assets/highlight/highlight.pack.js"></script>
            <script>hljs.initHighlightingOnLoad();</script>
            <script src="assets/js/scripts.js"></script>
        {% endblock %}
        {{ debugbar('render')|raw }}        
    </body>
</html>